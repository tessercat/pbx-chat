{
  "name": "pbx-ui",
  "version": "0.0.1",
  "description": "PBX UI",
  "main": "index.js",
  "scripts": {
    "css:common:gzip": "gzip dist/css/common/*.?????.css",
    "css:common:clean": "rimraf dist/css/common/*",
    "css:common:dev:sass": "node-sass src/scss/common.scss dist/css/common/dev.pre.css --output-style expanded",
    "css:common:dev:post": "postcss dist/css/common/dev.pre.css --config postcss/dev --no-map --output dist/css/common/dev.post.css",
    "css:common:dev:build": "npm run css:common:dev:sass && npm run css:common:dev:post",
    "css:common:dist:sass": "node-sass src/scss/common.scss dist/css/common/dist.pre.css --output-style compressed",
    "css:common:dist:post": "postcss dist/css/common/dist.pre.css --config postcss/dist --no-map --output dist/css/common/common.css",
    "css:common:dist:build": "npm run css:common:dist:sass && npm run css:common:dist:post",
    "css:common:build": "npm run css:common:clean && npm run css:common:dev:build && npm run css:common:dist:build && npm run css:common:gzip",
    "css:client:gzip": "gzip dist/css/client/*.?????.css",
    "css:client:clean": "rimraf dist/css/client/*",
    "css:client:dev:sass": "node-sass src/scss/client.scss dist/css/client/dev.pre.css --output-style expanded",
    "css:client:dev:post": "postcss dist/css/client/dev.pre.css --config postcss/dev --no-map --output dist/css/client/dev.post.css",
    "css:client:dev:build": "npm run css:client:dev:sass && npm run css:client:dev:post",
    "css:client:dist:sass": "node-sass src/scss/client.scss dist/css/client/dist.pre.css --output-style compressed",
    "css:client:dist:post": "postcss dist/css/client/dist.pre.css --config postcss/dist --no-map --output dist/css/client/client.css",
    "css:client:dist:build": "npm run css:client:dist:sass && npm run css:client:dist:post",
    "css:client:build": "npm run css:client:clean && npm run css:client:dev:build && npm run css:client:dist:build && npm run css:client:gzip",
    "css:build": "npm run css:common:build && npm run css:client:build",
    "js:clean": "rimraf dist/js/*",
    "js:gzip": "gzip dist/js/*.?????.js",
    "js:webpack": "webpack",
    "js:build": "npm run js:clean && npm run js:webpack && npm run js:gzip",
    "clean": "npm run css:common:clean && npm run css:client:clean && npm run js:clean",
    "build": "npm run css:build && npm run js:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tessercat/pbx-ui.git"
  },
  "author": "Peter Christensen",
  "license": "CC-BY-NC-ND-4.0",
  "bugs": {
    "url": "https://github.com/tessercat/pbx-ui/issues"
  },
  "homepage": "https://github.com/tessercat/pbx-ui#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "clean-webpack-plugin": "*",
    "compression-webpack-plugin": "^7.1.2",
    "copy-webpack-plugin": "^8.1.1",
    "eslint": "^7.25.0",
    "gzip-cli": "^1.2.0",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.14",
    "postcss-cli": "^8.3.1",
    "postcss-hash": "^0.2.1",
    "rimraf": "^3.0.2",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0",
    "webrtc-adapter": "^8.0.0"
  }
}
